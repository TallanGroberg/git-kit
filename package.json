{
  "name": "git-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "craco start",
    "build": "INLINE_RUNTIME_CHUNK=false craco build",
    "package": "INLINE_RUNTIME_CHUNK=false craco build && ts-node --esm ./tools/package-extension.script.ts",
    "eject": "craco eject",
    "test-e2e": "jest ./src/__test__/e2e",
    "test-unit": "craco test ./src/__test__/unit --watchAll=false",
    "test": "npm run test-unit && npm run test-e2e"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@radix-ui/colors": "^3.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.58",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "clsx": "^2.0.0",
    "craco": "^0.0.3",
    "dotenv": "^16.3.1",
    "lucide-react": "^0.288.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.3",
    "sinon-chrome": "^3.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.5.8",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/puppeteer": "^7.0.4",
    "@types/sinon-chrome": "^2.2.14",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "puppeteer": "^21.5.2",
    "ts-jest": "^29.1.1"
  }
}
